{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": "warn",
    "no-use-before-define": "warn",
    "spaced-comment": "warn",
    "semi": "error",
    "quotes": "error"
  },
  "extends": ["standard", "prettier", "prettier/standard"],
  "plugins": ["import", "prettier", "standard"]
}
